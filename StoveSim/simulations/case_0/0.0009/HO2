FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0.0009";
    object      HO2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    outlet_LHS
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    }
    outlet_RHS
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    }
    secondary_air_RHS
    {
        type            fixedValue;
        value           uniform 0;
    }
    secondary_air_LHS
    {
        type            fixedValue;
        value           uniform 0;
    }
    primary_inlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    stove_body
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
    defaultFaces
    {
        type            empty;
    }
}


