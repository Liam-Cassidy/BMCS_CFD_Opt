FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0.0001";
    object      rDeltaT;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 10000;

boundaryField
{
    outlet_LHS
    {
        type            extrapolatedCalculated;
        value           uniform 10000;
    }
    outlet_RHS
    {
        type            extrapolatedCalculated;
        value           uniform 10000;
    }
    secondary_air_RHS
    {
        type            extrapolatedCalculated;
        value           uniform 10000;
    }
    secondary_air_LHS
    {
        type            extrapolatedCalculated;
        value           uniform 10000;
    }
    primary_inlet
    {
        type            extrapolatedCalculated;
        value           uniform 10000;
    }
    stove_body
    {
        type            extrapolatedCalculated;
        value           uniform 10000;
    }
    frontAndBack
    {
        type            empty;
    }
    defaultFaces
    {
        type            empty;
    }
}


