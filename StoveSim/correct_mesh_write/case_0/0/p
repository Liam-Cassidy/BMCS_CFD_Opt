FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{
    primary_inlet
    {
        type            zeroGradient;
    }
	secondary_air_RHS
    {
        type            zeroGradient;
    }
	secondary_air_LHS
    {
        type            zeroGradient;
    }
    outlet_LHS
    {
        type            totalPressure;
        p0              $internalField;
    }
	outlet_RHS
    {
        type            totalPressure;
        p0              $internalField;
    }
    stove_body
    {
        type            zeroGradient;
    }
	pot_surfaces
    {
        type            zeroGradient;
    }
}


