{
    primary_inlet
    {
        type            fixedValue;
        value           uniform 0;
    }
	secondary_air_RHS
    {
        type            fixedValue;
        value           uniform 0.763149;
    }
	secondary_air_LHS
    {
        type            fixedValue;
        value           uniform 0.763149;
    }
    outlet_LHS
    {
        type            inletOutlet;
        inletValue      uniform 0.763149;
        value           uniform 0.763149;
    }
	outlet_RHS
    {
        type            inletOutlet;
        inletValue      uniform 0.763149;
        value           uniform 0.763149;
    }
    stove_body
    {
        type            zeroGradient;
    }
	pot_surfaces
    {
        type            zeroGradient;
    }
}


